{
  "baseBranches": ["dev"],
  "dependencyDashboard": true,
  "labels": ["dependencies"],
  "extends": [
    "config:base"
  ],
  "reviewers": [
    "RainbowDashLabs"
  ],
  "prConcurrentLimit": 3,
  "prHourlyLimit": 10,
  "packageRules": [
    {
      "groupName": "log4j",
      "matchPackagePrefixes": ["org.apache.logging.log4j"]
    },
    {
      "groupName": "sadu",
      "matchPackagePrefixes": ["de.chojo.sadu"]
    }
  ]
}
